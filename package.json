{
  "name": "glitch-api",
  "version": "1.2.0",
  "description": "Library for using Glitch API ⚙️",
  "main": "src/index",
  "scripts": {
    "start": "gitbook serve",
    "test": "jest tests/ --verbose",
    "docs:prepare": "gitbook install",
    "docs:api": "jsdoc2md --partial tools/docs/header --global-index-format none --files src/*/*  > docs/api.md",
    "docs:build": "npm run docs:prepare && npm run docs:api && gitbook build",
    "docs:serve": "npm run docs:prepare && npm run docs:api && gitbook serve",
    "docs:clean": "rimraf _book"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://glapi.glitch.me",
  "repository": {
    "url": "https://github.com/jarvis394/glitch-api/"
  },
  "files": [
    "src/"
  ],
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "js",
    "api"
  ],
  "devDependencies": {
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "gitbook-cli": "^2.3.2",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "jsdoc-md": "^3.0.0",
    "jsdoc-to-markdown": "^5.0.1",
    "path": "^0.12.7"
  }
}
